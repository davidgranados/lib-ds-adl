{
  "name": "@avaldigitallabs/bpop-hipotecario-lib-ds",
  "version": "1.0.0",
  "main": "src/lib.js",
  "files": [
    "/src"
  ],
  "scripts": {
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.js\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.js\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.js\"",
    "lint:css:prettier:fix": "yarn lint:css:prettier --write",
    "lint:css:prettier": "prettier '**/*.css' --list-different --ignore-path .gitignore",
    "lint:css:stylelint:fix": "yarn lint:css:stylelint --fix",
    "lint:css:stylelint": "stylelint '**/*.css' --ignore-path .gitignore",
    "lint:css": "run-s lint:css:stylelint lint:css:prettier",
    "lint:css:fix": "yarn lint:css:prettier:fix && yarn lint:css:stylelint:fix",
    "lint:ts": "eslint --cache --ignore-path .gitignore '**/*.ts*'",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:js": "eslint --cache --ignore-path .gitignore '**/*.js*'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:json:fix": "yarn lint:json --write",
    "lint:json": "prettier '**/*.json' --list-different --ignore-path .gitignore",
    "lint:all": "run-p lint:js lint:ts lint:json lint:css",
    "lint:all:fix": "run-p lint:ts:fix lint:js:fix lint:json:fix lint:css:fix",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "start": "react-scripts start",
    "test:jest": "react-scripts test --coverage",
    "test": "nyc --reporter=lcov mocha test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build:cra": "react-scripts build",
    "build:storybook": "build-storybook -s public",
    "build:tokens": "style-dictionary build -c ./src/tokens/config.json",
    "prebuild": "rm -rf dist",
    "build": "echo 'DONE'"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "chalk": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.1.2",
    "validator": "^13.6.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "js-isort": "^3.2.0",
    "jshint": "^2.13.0",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "repack-zip": "^0.2.8",
    "sonar-scanner": "^3.1.0",
    "style-dictionary": "^3.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^22.0.0"
  }
}
