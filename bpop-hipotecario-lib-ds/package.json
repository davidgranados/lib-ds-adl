{
  "name": "@avaldigitallabs/bpop-hipotecario-lib-ds",
  "version": "1.4.2",
  "license": "MIT",
  "private": false,
  "main": "./dist/lib.js",
  "module": "./dist/lib.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.ts\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.ts\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.ts\"",
    "lint:css:prettier:fix": "yarn lint:css:prettier --write",
    "lint:css:prettier": "prettier '**/*.css' --list-different --ignore-path .gitignore",
    "lint:css:stylelint:fix": "yarn lint:css:stylelint --fix",
    "lint:css:stylelint": "stylelint '**/*.css' --ignore-path .gitignore --cache",
    "lint:css": "run-s lint:css:stylelint lint:css:prettier",
    "lint:css:fix": "yarn lint:css:prettier:fix && yarn lint:css:stylelint:fix",
    "lint:ts": "eslint --cache --ignore-path .gitignore '**/*.ts*'",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:js": "eslint --cache --ignore-path .gitignore '**/*.js*'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:json:fix": "yarn lint:json --write",
    "lint:json": "prettier '**/*.json' --list-different --ignore-path .gitignore",
    "lint:all": "run-p lint:js lint:ts lint:json lint:css",
    "lint:all:fix": "run-p lint:ts:fix lint:js:fix lint:json:fix lint:css:fix",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "start": "react-scripts start",
    "test:watch": "react-scripts test",
    "test:watch:coverage": "react-scripts test --coverage --collectCoverageFrom=**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.stories.tsx --collectCoverageFrom=!src/**/index.ts --collectCoverageFrom=!src/**/*.d.ts --watchAll=true",
    "test": "react-scripts test --coverage --collectCoverageFrom=**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.stories.tsx --collectCoverageFrom=!src/**/index.ts --collectCoverageFrom=!src/**/*.d.ts --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build:cra": "react-scripts build",
    "build:storybook": "build-storybook -s public",
    "prebuild": "rm -rf dist",
    "build:lib": "rollup -c",
    "build": "run-s prebuild build:lib",
    "check:pipeline": "run-s lint:all:fix lint prettier test build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-image": "^2.1.0",
    "@rollup/plugin-node-resolve": "^13.0.1",
    "@rollup/plugin-typescript": "^8.2.3",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-to-json": "^1.5.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "js-isort": "^3.2.0",
    "jshint": "^2.13.0",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss": "^8.3.5",
    "postcss-url": "^10.1.3",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "repack-zip": "^0.2.8",
    "rollup": "^2.53.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sonar-scanner": "^3.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
