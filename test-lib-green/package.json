{
  "name": "@avaldigitallabs/test-lib-green",
  "version": "1.0.0",
  "main": "src/index.js",
  "files": [
    "/src"
  ],
  "scripts": {
    "test": "nyc --reporter=lcov mocha test",
    "prebuild": "rm -rf dist",
    "audit": "npm audit",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*.js\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*.js\"",
    "prettier": "prettier --check  \"./{test,src}/**/*.js\"",
    "sonar": "sonar-scanner -Dsonar.organization=$SONAR_ORGANIZATION -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN",
    "build": "echo 'DONE'"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chai-string": "^1.5.0",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jshint": "^2.10.2",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "repack-zip": "^0.2.5",
    "sonar-scanner": "^3.1.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "validator": "^13.5.2"
  }
}
